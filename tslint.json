{
  "extends": ["tslint:recommended"],
  "linterOptions": {
    "exclude": ["**/node_modules/**", "@types/**"]
  },
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "semicolon": [true, "never", "ignore-interfaces"],
    "trailing-comma": [true, "multi-line"]
  },
  "rulesDirectory": []
}
