@import 'sass/_true.styl'

+describe('Koffie')
  +it('should equal')
    assert-equal('10px', '10px')
  +it('should2 unequal')
    assert-unequal('20px', '20px')
  +it('should3 true')
    assert-true('20px', '20px')

+describe('Koffie2')
